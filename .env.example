# ============================================
# MoneyTicket 環境変数テンプレート (.env.example)
# ============================================

# 本番環境では以下の値を適切に設定してください

# Supabase設定
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Next.js用（同じ値を重複設定）
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# セキュリティキー（32文字ランダム）
ENCRYPTION_KEY=your-encryption-key-32-characters
JWT_SECRET=your-jwt-secret-32-characters
SESSION_SECRET=your-session-secret-32-characters

# Twilio設定（SMS送信用）
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=your-twilio-phone-number

# GitHub Actions用
GITHUB_TOKEN=${{ secrets.GITHUB_TOKEN }}

# セキュリティ設定
VITE_DEBUG=false
GENERATE_SOURCEMAP=false

# レート制限設定
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
SMS_RATE_LIMIT_MAX=5
AUTH_RATE_LIMIT_MAX=10

# セキュリティオプション
ENABLE_SECURITY_HEADERS=true
ENABLE_RATE_LIMITING=true
ENABLE_REQUEST_LOGGING=true
ENABLE_CORS_STRICT=true

# ログ設定
LOG_LEVEL=info
LOG_FILE_ERROR=logs/error.log
LOG_FILE_COMBINED=logs/combined.log

# 自動デプロイ設定
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-vercel-org-id
VERCEL_PROJECT_ID=your-vercel-project-id

# 環境設定
NODE_ENV=production

# AI/MCP関連（オプション）
GEMINI_API_KEY=your-gemini-api-key

# アプリケーション情報
VITE_APP_TITLE=MoneyTicket
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=資産運用診断アプリケーション
