# 環境変数の例
# 本番環境では必ず新しい値を生成してください

# Supabase設定（公開可能）
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# サーバーサイドのみ（VITE_プレフィックスなし）
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
ENCRYPTION_KEY=generate-with-crypto-randomBytes
JWT_SECRET=generate-with-crypto-randomBytes
SESSION_SECRET=generate-with-crypto-randomBytes
CSRF_SECRET=generate-with-crypto-randomBytes

# Twilio設定（サーバーサイドのみ）
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# 管理者設定（サーバーサイドのみ）
ADMIN_USERNAME=admin
ADMIN_PASSWORD_HASH=bcrypt-hash

# その他の設定
NODE_ENV=production
PORT=3000