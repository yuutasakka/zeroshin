{
  "name": "taskal-app",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:clean": "node clear-dev-cache.js && vite",
    "dev:server": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build",
    "build:clean": "node clear-dev-cache.js && npm run build",
    "start": "vite preview --host --port 4173",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 1000 || exit 0",
    "type-check": "tsc --noEmit || exit 0",
    "test": "vitest run || echo 'No tests configured'",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "generate-keys": "node scripts/generate-env-keys.js",
    "update-supabase-data": "tsx scripts/update-supabase-data.ts",
    "setup-env": "mkdir -p scripts && node scripts/generate-env-keys.js",
    "validate-production": "node scripts/validate-production-env.js",
    "build:production": "npm run validate-production && npm run build",
    "security-check": "npm run validate-production",
    "check-env": "node scripts/check-production-env.js",
    "generate-credentials": "node scripts/generate-production-credentials.js",
    "security:scan": "node scripts/security-checklist.ts",
    "pre-deploy": "npm run check-env && npm run security-check",
    "release:staging": "npm run pre-deploy && npm run build",
    "release:production": "npm run pre-deploy && npm run build:production",
    "audit:fix": "npm audit fix --force",
    "audit:report": "npm audit --audit-level=moderate",
    "deploy:staging": "vercel --scope=team",
    "deploy:production": "vercel --prod --scope=team",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "npm run build && npm run preview & cypress run && kill %1",
    "lighthouse": "lhci autorun",
    "clean": "rm -rf dist node_modules/.vite .eslintcache",
    "reinstall": "npm run clean && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@sentry/react": "^8.42.0",
    "@supabase/supabase-js": "^2.39.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "qrcode.react": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "twilio": "^5.7.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.16.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.21.1",
    "express-openapi-validator": "^5.5.7",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "swagger-ui-express": "^5.0.1",
    "terser": "^5.43.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0",
    "yamljs": "^0.3.0"
  }
}
