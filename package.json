{
  "name": "money-ticket-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "esbuild": "^0.25.6",
    "path-to-regexp": "^6.3.0",
    "undici": "^5.28.6"
  },
  "scripts": {
    "dev": "vite",
    "dev:clean": "node clear-dev-cache.js && vite",
    "dev:server": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "vite build",
    "build:clean": "node clear-dev-cache.js && npm run build",
    "start": "vite preview --host --port 4173",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "generate-keys": "node scripts/generate-env-keys.js",
    "setup-env": "mkdir -p scripts && node scripts/generate-env-keys.js",
    "validate-production": "node scripts/validate-production-env.js",
    "build:production": "npm run validate-production && npm run build",
    "security-check": "npm run validate-production",
    "check-env": "node scripts/check-production-env.js",
    "generate-credentials": "node scripts/generate-production-credentials.js",
    "security:scan": "node scripts/security-checklist.ts",
    "pre-deploy": "npm run check-env && npm run security-check",
    "release:staging": "npm run pre-deploy && npm run build",
    "release:production": "npm run pre-deploy && npm run build:production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:component": "cypress run --component",
    "test:all": "npm run test && npm run test:e2e",
    "production:env": "node scripts/check-production-env.js",
    "production:setup": "node scripts/generate-production-credentials.js",
    "vulnerability:scan": "node scripts/vulnerability-scan.js",
    "git-history": "./scripts/git-history-viewer.sh",
    "git-diff": "./scripts/git-diff-viewer.sh",
    "file-monitor": "node scripts/file-change-monitor.js",
    "dev-tools": "docker-compose up -d"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.19",
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.50.2",
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.17.6",
    "@types/node-fetch": "^2.6.12",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/mcp-adapter": "^0.11.1",
    "@vercel/node": "^2.3.0",
    "@vitejs/plugin-react": "^4.6.0",
    "ai": "^4.3.16",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "papaparse": "^5.5.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "twilio": "^5.7.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.44.1"
  }
}
