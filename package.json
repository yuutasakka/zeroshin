{
  "name": "taskal-app",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run check-env && npm run security-check && vite build && node build-security.mjs",
    "preview": "vite preview",
    "security-check": "node env-check.js",
    "check-env": "node env-check.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "verify": "npm run lint && npm run format && npm run test",
    "clean": "rm -rf node_modules dist",
    "reinstall": "npm run clean && npm install --legacy-peer-deps",
    "monitor": "node --loader ts-node/esm server/monitoring/cli-monitor.ts",
    "monitor:dashboard": "npm run monitor dashboard",
    "monitor:status": "npm run monitor status",
    "monitor:anomalies": "npm run monitor anomalies",
    "monitor:cleanup": "npm run monitor cleanup"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@sentry/react": "^8.42.0",
    "@supabase/supabase-js": "^2.39.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "qrcode.react": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "twilio": "^5.7.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.2.1",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1",
    "workbox-window": "^7.3.0"
  }
}